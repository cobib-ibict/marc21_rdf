<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:vivo="http://vivoweb.org/ontology/core#"
  xmlns:bibo="http://purl.org/ontology/bibo/"
  xmlns:vitro="http://vitro.mannlib.cornell.edu/ns/vitro/0.7#"
  xmlns:dcterms="http://purl.org/dc/terms/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:obo="http://purl.obolibrary.org/obo/"
>
  <!-- Publisher -->
<rdf:Description rdf:about="{{ article.publisher.url }}">
    <rdf:type rdf:resource="http://vivoweb.org/ontology/core#Publisher" />
    <rdfs:label xml:lang="{{ article.language }}">{{ article.publisher.first_name }} {{ article.publisher.last_name }}</rdfs:label>
    <vivo:firstName>{{ article.publisher.first_name }}</vivo:firstName>
    <vivo:lastName>{{ article.publisher.last_name }}</vivo:lastName>
    <vivo:publisherOf rdf:resource="{{ article.url }}" />
  </rdf:Description>

  <!-- Article -->
  <vivo:Article rdf:about="{{ article.url }}">
    <rdfs:label
      xml:lang="{{ article.language }}">{{ article.subject }}</rdfs:label>
    <vivo:Title>{{ article.subject }}</vivo:Title>
    <vivo:freetextKeyword
    xml:lang="{{ article.language }}">{{ article.subject }}
    </vivo:freetextKeyword> {% for word in article.subject.split(' ') %} <vivo:freetextKeyword xml:lang="{{ article.language }}">{{word}}</vivo:freetextKeyword> {% endfor %}
    {% if article.published_at %}
      <vivo:dateTime>{{ article.published_at.isoformat()[:-7] }}</vivo:dateTime>
      <vivo:dateTimeValue>{{ article.published_at.isoformat()[:-7] }}</vivo:dateTimeValue>
    {% endif %}
  </vivo:Article>

  <!-- Article -->
  <bibo:Article rdf:about="{{ article.url }}">
    <bibo:authorList rdf:parseType="Resource">
      <bibo:author rdf:resource="{{ article.publisher.url }}" />
    </bibo:authorList>
    <bibo:title>{{ article.subject }}</bibo:title>
    {% if article.abstract %}
    <bibo:abstract xml:lang="{{ article.language }}">{{ article.abstract }}</bibo:abstract>
    {% endif %}
    {% if article.published_at %}
      <vivo:dateTime>{{ article.published_at.isoformat()[:-7] }}</vivo:dateTime>
      <vivo:dateTimeValue>{{ article.published_at.isoformat()[:-7] }}</vivo:dateTimeValue>
    {% endif %}
  </bibo:Article>
  <!-- City -->
  <rdf:Description rdf:about="{{ article.url }}">
    <rdf:type rdf:resource="http://vitro.mannlib.cornell.edu/ns/vitro/public#Location" />
    <vitro:locationName>{{ article.city }}</vitro:locationName>
    <vivo:placeOfPublication xml:lang="{{ article.language }}"> {{ article.city}} </vivo:placeOfPublication>
  </rdf:Description>
  <vivo:Authorship rdf:about="{{ article.author_ship }}">
    <vivo:relates rdf:resource="{{ article.publisher.url }}"/>
    <vivo:relates rdf:resource="{{ article.url }}"/>
  </vivo:Authorship>
</rdf:RDF>
